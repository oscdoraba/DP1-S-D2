<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<import resource="conversion-service.xml" />
	
	
	<!-- Shout -->
	
	<bean id="shout01" class="acme.entities.shout.Shout">
	
	<property name="author" value="John Doe"/>
	<property name="text" value="I´m happy"/>
	<property name="moment" value="2019/09/09 10:00"/>
	
	</bean>
	
	<bean id="shout02" class="acme.entities.shout.Shout">
	
	<property name="author" value="Jane Doe"/>
	<property name="text" value="I´m the master of my room!"/>
	<property name="moment" value="2019/08/09 15:55"/>
	
	</bean>
	
	<bean id="shout03" class="acme.entities.shout.Shout">
	
	<property name="author" value="Foo Bar"/>
	<property name="text" value="Just turning 20!"/>
	<property name="moment" value="2019/09/07 00:00"/>
	
	</bean>
	
	<!-- DoradoBulletin -->
	
	<bean id="doradoBulletin1" class="acme.entities.doradoBulletin.DoradoBulletin">
	
	<property name="sender" value="Oscar"/>
	<property name="receiver" value="Pablo"/>
	<property name="web" value="https://www.gmail.com"/>
	
	</bean>
	
	<!-- CalleBulletin -->
	
	<bean id="calleBulletin01" class="acme.entities.calleBulletin.CalleBulletin">
	
	<property name="title" value="Uno contra 20 Mc´s"/>
	<property name="artist" value="Toteking"/>
	<property name="album" value="Música para Enfermos"/>
	<property name="moment" value="2004/07/24 17:43"/>
	
	</bean>
	
	<bean id="calleBulletin02" class="acme.entities.calleBulletin.CalleBulletin">
	
	<property name="title" value="1000 guerras"/>
	<property name="artist" value="Ayax y Prok"/>
	<property name="album" value="Single"/>
	<property name="moment" value="2020/07/16 15:00"/>
	
	</bean>
	
	<bean id="calleBulletin03" class="acme.entities.calleBulletin.CalleBulletin">
	
	<property name="title" value="Y la felicidad qué"/>
	<property name="artist" value="Canserbero"/>
	<property name="album" value="Vida"/>
	<property name="moment" value="2010/07/08 12:34"/>
	
	</bean>
	
	<bean id="calleBulletin04" class="acme.entities.calleBulletin.CalleBulletin">
	
	<property name="title" value="Anochece/Manifiesto"/>
	<property name="artist" value="Nach"/>
	<property name="album" value="Un día en suburbia"/>
	<property name="moment" value="2008/02/13 03:34"/>
	
	</bean>
	
	<!-- Inquiries -->
	
	<bean id="inquiries01" class="acme.entities.inquiries.Inquiries">
	
	<property name="title" value="¿La sangre manda en castellano?"/>
	<property name="dateOfCreation" value="2020/05/01 11:32"/>
	<property name="deadline" value="2022/04/07 12:34"/>
	<property name="description" value="Hola, quería saber si ya se encuentra la versión traducida del nuevo libro de Stephen
	King titulado If It Bleeds en su idioma original
	
	Atentamente, un usuario inquieto"/>
	<property name="minMoney" value="€ 21.70"/>
	<property name="maxMoney" value="€ 22.70"/>
	<property name="email" value="impaciente@gmail.com"/>
	</bean>
	
	<!-- Technology Records -->
	
	<bean id="technologyRecords01" class="acme.entities.technologyRecords.TechnologyRecords">
	
	<property name="title" value="Corriente alterna"/>
	<property name="activitySector" value="Electricidad"/>
	<property name="inventor" value="Nikola Tesla"/>
	<property name="description" value="Combatió a la corriente continua de Thomas Alva Edison con éxito, funcionando en la actualidad."/>
	<property name="website" value="https://solar-energia.net/electricidad/corriente-electrica/corriente-alterna"/>
	<property name="email" value="ntvstae@gmail.com"/>
	<property name="indication" value="OPEN_SOURCE"/>
	<property name="stars" value="10"/>
	</bean>
	
	

	

</beans>
